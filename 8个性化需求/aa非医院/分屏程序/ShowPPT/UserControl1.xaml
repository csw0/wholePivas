<UserControl x:Name="UC1" x:Class="ShowPPT.ShowPT"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ShowPPT"
             mc:Ignorable="d" 
             d:DesignHeight="360" Width="640" Loaded="UserControl_Loaded" HorizontalAlignment="Left" VerticalAlignment="Top" Height="360">
    <Border x:Name="Bor1" BorderBrush="Wheat" BorderThickness="1" CornerRadius="10" ClipToBounds="True" SnapsToDevicePixels="True" Margin="0" MouseLeftButtonDown="Bor1_MouseLeftButtonDown" MouseRightButtonDown="Bor1_MouseRightButtonDown">
        <Border.Background>
            <ImageBrush/>
        </Border.Background>
    </Border>
</UserControl>
